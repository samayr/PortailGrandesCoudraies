            <Source Path="DBContext" DbContext="LesGrandesCoudraies.Models.ApplicationDbContext, LesGrandesCoudraies" Origin="Convention" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="ModelContainer" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="LesGrandesCoudraies.Models.ModelContainer, LesGrandesCoudraies" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String" />
